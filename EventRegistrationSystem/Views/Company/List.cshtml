@using EventRegistrationSystem.Models;
@model IEnumerable<Company>
@{
    ViewBag.Title = "Companies";
}
@foreach (var c in Model)
{
<div>
    <h3>@c.Name</h3>
    <h4>@c.Address</h4>
    <h4>@c.PhoneNumber</h4>
    <h4>@c.Email</h4>
    <h4>@c.PrimaryContactName</h4>
    <h4>@c.PrimaryContactPhoneNumber</h4>
    <h4>@c.PrimaryContactEmail</h4>
    <h4>@c.PrimaryContactName</h4>
    <h4>@c.CompanyWebLink</h4>
</div>
}